# filename: .env.example
# Caminhos base (pasta do projeto como raiz de execução)
DATA_DIR=./data
# Diretório para artefatos de dados (raw/interim/processed/external)

LOG_DIR=./logs
# Diretório para arquivos de log em JSON Lines

METADATA_DIR=./metadata
# Diretório para manifestos, hashes e proveniência

CONFIG_DIR=./config
# Diretório para schemas e configuração de logging

TZ=UTC
# Timezone padrão para registros e logs (usar UTC)

LOCALE=pt_BR.UTF-8
# Locale para formatação de textos; ponto como separador decimal no CSV

HTTP_PROXY=
HTTPS_PROXY=
NO_PROXY=localhost,127.0.0.1
# Proxies opcionais para acesso a recursos de rede (não incluir credenciais reais)

PYTHONUTF8=1
# Garante modo UTF-8 em execuções Python

PYTHONHASHSEED=0
# Semente determinística para hashing (reprodutibilidade)

REPO_NAME=desig-ineq-politica-br-participacao
# Nome do repositório (alterar se necessário)

CODE_VERSION=0.1.0
DATA_VERSION=0.1.0
# Versões iniciais pretendidas para código e dados (semver). Atualize ao criar tags.

LOG_LEVEL=INFO
# Nível padrão de logs: INFO | WARNING | ERROR

RUN_ID=
# Identificador opcional de execução preenchido por orquestração em etapas futuras
